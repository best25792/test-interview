meta {
  name: Get Wallet Balance
  type: http
  seq: 2
}

get {
  url: {{base_url}}/api/v1/users/{{userId}}/wallet/balance
  body: none
  auth: none
}

vars:pre-request {
  base_url: http://localhost:8080
  userId: 1
}

tests {
  test("Status code is 200", function() {
    expect(res.status).to.equal(200);
  });
  
  test("Response has balance", function() {
    expect(res.body.balance).to.exist;
  });
}
